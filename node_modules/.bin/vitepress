#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/vitedoc/node_modules/.pnpm/vitepress@1.0.0-alpha.66_react-dom@15.7.0_react@15.7.0/node_modules/vitepress/bin/node_modules:/data/data/com.termux/files/home/vitedoc/node_modules/.pnpm/vitepress@1.0.0-alpha.66_react-dom@15.7.0_react@15.7.0/node_modules/vitepress/node_modules:/data/data/com.termux/files/home/vitedoc/node_modules/.pnpm/vitepress@1.0.0-alpha.66_react-dom@15.7.0_react@15.7.0/node_modules:/data/data/com.termux/files/home/vitedoc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/vitedoc/node_modules/.pnpm/vitepress@1.0.0-alpha.66_react-dom@15.7.0_react@15.7.0/node_modules/vitepress/bin/node_modules:/data/data/com.termux/files/home/vitedoc/node_modules/.pnpm/vitepress@1.0.0-alpha.66_react-dom@15.7.0_react@15.7.0/node_modules/vitepress/node_modules:/data/data/com.termux/files/home/vitedoc/node_modules/.pnpm/vitepress@1.0.0-alpha.66_react-dom@15.7.0_react@15.7.0/node_modules:/data/data/com.termux/files/home/vitedoc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
